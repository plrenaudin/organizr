{
  "name": "organizr.io",
  "description": "Helps you organizing events",
  "version": "1.0.0",
  "author": "Pierre-Louis Renaudin <pl.renaudin@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:staging": "cross-env NODE_ENV=staging webpack --progress --hide-modules",
    "start": "MONGODB_URI=mongodb://localhost:27017/organizr node server/server.js",
    "test:server": "NODE_ENV=test MONGODB_URI=mongodb://localhost:27017/tests mocha test/server --exit",
    "test": "npm run test:server",
    "coverage": "NODE_ENV=test MONGODB_URI=mongodb://localhost:27017/tests istanbul cover _mocha -- test/server "
  },
  "dependencies": {
    "bunyan": "^1.8.9",
    "emailjs": "^1.0.9",
    "jsonwebtoken": "^8.1.0",
    "mongodb": "^2.2.25",
    "monk": "^6.0.5",
    "passwordless": "^1.1.2",
    "passwordless-mongostore": "^0.1.4",
    "request": "^2.81.0",
    "restify": "^6.3.4",
    "restify-jwt": "^0.4.0",
    "vue": "^2.2.6"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.2.6",
    "vuex": "^3.0.1",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.4.2",
    "zenscroll": "^4.0.0"
  }
}
