{
  "name": "organizr",
  "description": "Helps you organizing events",
  "version": "1.0.0",
  "author": "Pierre-Louis Renaudin <pl.renaudin@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:staging": "webpack --mode development",
    "start": "MONGODB_URI=mongodb://localhost:27017/organizr node server/server.js",
    "test:server": "NODE_ENV=test MONGODB_URI=mongodb://localhost:27017/tests mocha test/server --exit",
    "test": "yarn test:server",
    "coverage": "NODE_ENV=test MONGODB_URI=mongodb://localhost:27017/tests istanbul cover _mocha -- test/server",
    "lint": "eslint --ext js,vue  src server",
    "lint:fix": "eslint --ext js,vue --fix src server"
  },
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@babel/preset-env": "7.5.4",
    "core-js": "^3.1.4",
    "axios": "0.19.0",
    "babel-loader": "8.0.6",
    "chai": "4.2.0",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "eslint": "^6.0.1",
    "eslint-plugin-vue": "5.2.3",
    "file-loader": "4.0.0",
    "font-awesome": "4.7.0",
    "html-webpack-plugin": "3.2.0",
    "istanbul": "0.4.5",
    "mocha": "6.1.4",
    "node-sass": "4.12.0",
    "prettier": "1.11.1",
    "prettier-eslint": "9.0.0",
    "regenerator-runtime": "^0.13.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "2.0.1",
    "vue": "2.6.10",
    "vue-loader": "15.7.0",
    "vue-router": "3.0.7",
    "vue-template-compiler": "2.6.10",
    "vuex": "3.1.1",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "zenscroll": "4.0.2"
  },
  "prettier": {
    "eslintIntegration": true
  }
}
